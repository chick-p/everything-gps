name = "everything-gps-scheduler"
compatibility_date = "2024-04-05"
main = "src/index.ts"

[[d1_databases]]
binding = "DB"
database_name = "everything-gps-database"
database_id = "<YOUR_DB_ID>"
migrations_dir = "drizzle/migrations"

[vars]
SLACK_WEBHOOK_URL = "<YOUR_WEBHOOK_URL>"
EVERYTHING_GPS_WEB_URL = "<YOUR_WEB_URL>"

# every Saturday at 9am
[triggers]
crons = ["0 9 * * 6"]
